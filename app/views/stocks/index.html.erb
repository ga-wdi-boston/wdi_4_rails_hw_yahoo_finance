<h1>Top Ten Tech Stocks: Summary</h1>

<dl>
  <% @stocks_summary.each do |stock| %>
  <dt><%= stock.symbol %></dt>
  <dd>
  Last Trade Price: <%= stock.last_trade_price %><br>
  52 Week High: <%= stock.high_52_weeks %><br>
  52 Week Low: <%= stock.low_52_weeks %><br>
  <%= link_to 'Additional Details', stocks_show_path(stock.symbol) %>
  </dd>
  <% end %>
</dl>

